{
    "name": "clojurelinter",
    "displayName": "ClojureLinter",
    "description": "Simple linter extension for Clojure",
    "version": "0.0.1",
    "publisher": "maxlorenz",
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:clojure"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "languages": [
            {
                "id": "clojure",
                "aliases": [
                    "Clojure",
                    "clojure"
                ],
                "extensions": [
                    ".clj",
                    ".cljc"
                ]
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}